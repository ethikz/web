{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [
      true, {
        "ignoreAtRules": [
          "extends",
          "ignores"
        ]
      }
    ],
    "indentation": "tab",
    "number-leading-zero": null,
    "unit-whitelist": [
      "px",
      "em",
      "rem",
      "s",
      "%"
    ]
  }
}
